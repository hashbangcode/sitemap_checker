{
    "name": "hashbangcode/sitemap_checker",
    "type": "project",
    "license": "MIT",
    "description": "A PHP library used to download, parse and crawl sitemap.xml files.",
    "autoload": {
        "psr-4": {
            "Hashbangcode\\SitemapChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hashbangcode\\SitemapChecker\\Test\\": "tests/"
        },
        "files": ["app/AppKernel.php"]
    },
    "authors": [
        {
            "name": "Philip Norton",
            "email": "hello@hashbangcode.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.5",
        "symfony/console": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/yaml": "^7.0",
        "guzzlehttp/promises": "^2.0",
        "symfony/framework-bundle": "^7.0",
        "chrome-php/chrome": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "test" : "./vendor/bin/phpunit",
        "test-coverage": "./vendor/bin/phpunit -c phpunit.coverage.xml",
        "phpstan": "./vendor/bin/phpstan"
    }
}
