{
    "name": "hashbangcode/sitemap_checker",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Hashbangcode\\SitemapChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hashbangcode\\SitemapChecker\\Test\\": "tests/"
        },
        "files": ["app/AppKernel.php"]
    },
    "authors": [
        {
            "name": "Philip Norton",
            "email": "philipnorton42@gmail.com"
        }
    ],
    "require": {
        "chrome-php/chrome": "^1.8",
        "guzzlehttp/guzzle": "^7.5",
        "symfony/console": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/config": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/yaml": "^6.2",
        "guzzlehttp/promises": "^1.5",
        "symfony/framework-bundle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "test" : "./vendor/bin/phpunit",
        "test-coverage": "./vendor/bin/phpunit -c phpunit.coverage.xml",
        "phpstan": "./vendor/bin/phpstan"
    }
}
